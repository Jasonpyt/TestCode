<?xml version="1.0" encoding="UTF-8" ?>
<!--
   使用提供的dtd文档约束  bean.dtd
      1：从DTD文档中复制一个标签。
      2：根据提示编写xml文档。

      <!ELEMENT
         对元素标签的 约束

      <!ATTLIST
         对属性的约束

-->
<!--
     beans 就代表当前xml 的根标签
     SYSTEM 当前文档的引入 是本地引入
     "bean.dtd"  引入文档的位置
 -->
<!DOCTYPE beans SYSTEM "bean.dtd">
<beans>
    <bean id="b001" className="myclass">
         <property name="" value=""></property>
     </bean>

    <bean id="b001" className="myclass">
        <property name="" value=""></property>
    </bean>
    <bean id="b001" className="myclass">
         <property name="" value=""></property>
    </bean>
    <import resource=""></import>
</beans>